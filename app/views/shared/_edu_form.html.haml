= simple_form_for(@education, remote: true) do |f|
  = f.error_notification

  .form-inputs
    = f.input :institute_name
    = f.input :course_name
    = f.select(:start_date, options_for_select(["2007","2008","2009"],   "Start date:"))
    = f.select(:end_date, options_for_select(["2011","2012","2013"],   "end date:"))
  .form-actions-edu
    = f.button :submit , value: "Add Education"
    = button_tag "Cancel", id: "cancel-edu", type:"button"
